<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="283.0" prefWidth="240.0" spacing="20.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aydin.firebasedemospring2024.WelcomeController">
    <children>
        <Label text="Welcome!" />
      <TextField fx:id="emailField" promptText="Email" />
      <PasswordField fx:id="passwordField" promptText="Password" />
        <Button fx:id="loginButton" onAction="#loginButtonClicked" prefHeight="25.0" prefWidth="100.0" text="Login" />
      <Label fx:id="registerLabel" onMouseClicked="#registerButtonClicked" text="No account? Register here." />
      <Label fx:id="noticeLabel" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
